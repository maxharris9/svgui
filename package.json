{
  "name": "svgui",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "start": "budo src/index.js --live -- -t [ bubleify --extensions .js --objectAssign merge --jsx React.createElement ] -t [ aliasify ] -g [ envify --NODE_ENV production ] -g uglifyify",
    "start-dev": "budo src/index.js --live -- -t [ bubleify --extensions .js --objectAssign merge --jsx React.createElement ] -t [ aliasify ] -g [ envify --NODE_ENV development ]",
    "prod": "budo src/index.js -- -g [ envify --NODE_ENV production ] -t [ aliasify --extensions .js ]",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "standard",
    "babbabable": "babel --plugins @babel/plugin-transform-react-jsx src --out-dir lib",
    "buble-start": "budo lib/index.js",
    "buble": "buble -i src/ -o lib/ --objectAssign merge --jsx React.createElement --target chrome:63",

    "boozerify": "browserify lib/test/undom.js lib/test/undom.js -d -o bundle.js -t [ aliasify ]"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxharris9/svgui.git"
  },
  "author": "maxharris9",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/maxharris9/svgui/issues"
  },
  "homepage": "https://github.com/maxharris9/svgui#readme",
  "aliasify": {
    "aliases": {
      "react": "preact-compat",
      "react-dom": "preact-compat"
    }
  },
  "dependencies": {
    "babelify": "10.0.0",
    "budo": "^11.3.2",
    "esprima": "^4.0.1",
    "estraverse": "^4.2.0",
    "estraverse-fb": "^1.3.2",
    "lodash.debounce": "^4.0.8",
    "lodash.merge": "^4.6.1",
    "preact": "^8.3.1",
    "preact-compat": "^3.18.4",
    "preact-context": "^1.1.1",
    "undom": "^0.4.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.1",
    "@babel/plugin-transform-react-jsx": "7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "aliasify": "^2.1.0",
    "buble": "^0.19.3",
    "bubleify": "^1.2.0",
    "envify": "^4.1.0",
    "jsdom": "^12.0.0",
    "standard": "^11.0.1",
    "svgdom": "0.0.15",
    "uglify-js": "^3.4.9",
    "uglifyify": "^5.0.1"
  }
}
